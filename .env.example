# ----------------------------------------------------------------------------
# COPILOT STUDIO AGENT - WEB AGENT
# ----------------------------------------------------------------------------
# Configuraci贸n para el agente web de Copilot Studio
COPILOTSTUDIOAGENT__ENVIRONMENTID="Default-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
COPILOTSTUDIOAGENT__TENANTID="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
COPILOTSTUDIOAGENT__AGENTAPPID="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
COPILOTSTUDIOAGENT__SCHEMANAME="cr56a_yourAgentSchemaName"

# ----------------------------------------------------------------------------
# COPILOT STUDIO AGENT - EMAIL AGENT
# ----------------------------------------------------------------------------
# Configuraci贸n para el agente de email de Copilot Studio
COPILOTSTUDIOAGENTMAIL__ENVIRONMENTID="Default-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
COPILOTSTUDIOAGENTMAIL__TENANTID="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
COPILOTSTUDIOAGENTMAIL__AGENTAPPID="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
COPILOTSTUDIOAGENTMAIL__SCHEMANAME="cr56a_yourMailAgentSchemaName"

# ----------------------------------------------------------------------------
# AZURE OPENAI
# ----------------------------------------------------------------------------
# Configuraci贸n para Azure OpenAI Service
AZURE_OPENAI_CHAT_DEPLOYMENT_NAME="gpt-4o"
AZURE_OPENAI_API_KEY="your_azure_openai_api_key_here"
AZURE_OPENAI_ENDPOINT="https://your-resource-name.openai.azure.com/"

# ----------------------------------------------------------------------------
# AZURE AI FOUNDRY (Azure AI Studio)
# ----------------------------------------------------------------------------
# Configuraci贸n para Azure AI Foundry / Azure AI Studio
AZURE_AI_PROJECT_ENDPOINT="https://your-resource.services.ai.azure.com/api/projects/your-project-name"
AZURE_AI_MODEL_DEPLOYMENT_NAME="gpt-4o"

# ----------------------------------------------------------------------------
# TELEMETRY & OBSERVABILITY
# ----------------------------------------------------------------------------
# Habilitar OpenTelemetry para trazabilidad
ENABLE_OTEL=true
